{
	"name": "csie.io DDNS",
	"description": "csie.io DDNS client for chrome. 本服務由csie.io提供, 詳細說明請登入: https://csie.io DNS管理介面",
	"version": "0.0.1",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": ["tabs", "storage", "alarms", "https://csie.io/*"],
	"browser_action": {
		"default_title": "csie.io DDNS",
		"default_icon": "icon.png"
	},
	"icons": { 
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png" },
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"options_page": "option.html",
  	"manifest_version": 2
}
