{
	"name": "csie.io DDNS",
	"description": "csie.io DDNS client for chrome. 本服務由csie.io提供, 詳細說明請登入: https://csie.io DNS管理介面",
	"version": "1.0.0.2",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": ["tabs", "notifications", "storage", "alarms", "https://csie.io/*"],
	"browser_action": {
		"default_title": "csie.io DDNS",
		"default_icon": "img/csieio-32.png"
	},
	"icons": { 
		"16": "img/csieio-16.png",
		"48": "img/csieio-48.png",
		"128": "img/csieio-128.png" },
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"options_page": "option.html",
  	"manifest_version": 2
}
